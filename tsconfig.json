{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "sourceMap": true,
    "outDir": "lib",
    "rootDir": "./src",
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noImplicitAny": false,
    "lib": [
      "es2015"
    ]
  },
  "include": [
    "src/*"
  ]
}
