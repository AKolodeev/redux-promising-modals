{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "quotemark": {
            "options": [
                "single",
                "avoid-escape"
            ]
        },
        "trailing-comma": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
